@{
    ViewBag.Title = "Admin Home";

}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/custom.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/custom.js"></script>
</head>

<body>

    <div>

        <nav class="navbar navbar-inverse">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="@Url.Action("Index","Admin")" style="font-size:x-large">Pucit Radio</a>
                </div>

                <ul class="nav navbar-nav navbar-right">

                    <li><a href="/Admin/Index" id="b0"><span class="glyphicon glyphicon-music"></span> Home</a></li>
                    <li><a href="/User/Index"><span class="glyphicon glyphicon-log-out"></span> LogOut</a></li>
                    
                    <li class="dropdown"><a href="#" id="b3" class="dropdown-toggle" data-toggle="dropdown" style="margin-left:5px">
                        <span class="glyphicon  caret"></span> Show Timing</a>
                        <ul class="dropdown-menu">
                            <li><a href="/Admin/PShowtiming" id="b8">Paid User</a></li>
                            <li><a href="/Admin/Showtiming" id="b9">Normal User</a></li>
                            
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" id="b14" class="dropdown-toggle" data-toggle="dropdown" style="margin-left:5px">
                            <span class="glyphicon  caret"></span> News
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="/Admin/Newsalert" id="b18">News Alert</a></li>
                            <li><a href="/Admin/imageBanner" id="b19">Image Banner</a></li>
                            
                        </ul>
                    </li>
                  
                    <li><a href="/Admin/Complain" id="b5">Complain</a></li>
                    <li><a href="#" id="b6">Mobile App</a></li>
                    <li><a href="/Admin/Showuser" id="b4"> User</a></li>
                    <li><a href="#" id="b7"> About Us</a></li>
                    
                </ul>
                
            
            </div>

        </nav>
    </div>


    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 img">

        <img class="img1" src="~/images/background.jpg" />
        <div id="div1">
            @RenderBody()
           
        </div>
    </div>
    <div class="clearfix"></div>
    <div id="footerOuterWrapper">
        <div id="footerWrapper">

            <div class="footer">


                <p>&nbsp;</p>


                <div class="row other-links center-block">
                    <div class="col-sm-3 col-xs-6">
                        <b><span class="glyphicon glyphicon-phone grey-circle" aria-hidden="true"></span>Contact us</b><br>
                        <p>
                            T: 000 000 0000<br>
                            E:<small> <a href="#">pucitradio@pucit.edu.pk</a></small><br>
                        </p>

                    </div>
                    <div class="col-sm-3 col-xs-6">
                        <b><span class="glyphicon glyphicon-map-marker grey-circle" aria-hidden="true"></span>Find us</b><br>
                        <p>
                            PUCIT RADIO<br>
                            Punjab University<br>
                            Old Campus <br>
                            Mall-Road Lahore<br>
                        </p>

                    </div>
                    <div class="col-sm-3 col-xs-6">
                        <b><span class="glyphicon glyphicon-info-sign grey-circle" aria-hidden="true"></span>Learn more</b><br>
                        <p>
                            <a href="#">Terms of Use</a><br>
                            <a href="#">Privacy Policy</a><br>
                            <a href="#">Blog</a><br>
                            <a href="#">FAQ</a>
                        </p>

                    </div>


                </div>
                <div id="copyright">
                    Copyright (c) ZK, 2016. All rights reserved. Reproduction in whole or in part without permission is prohibited.<br>
                    Pucit Radio- pu old campus mall road lahore - Company Number: 00000000 - Mob: 00000000<br>

                </div>

                <div class="row social-links center-block" style="max-width:230px;">

                    <div id="social12" class="col-xs-4 img-circle"><a href="https://www.facebook.com/" target="_blank"><img src="~/images/footer-fb.png" /></a></div>
                    <div id="social13" class="col-xs-4 img-circle"><a href="https://twitter.com/" target="_blank"><img src="~/images/footer-twitter.png" /></a></div>

                    <div id="social14" class="col-xs-4 img-circle"><a href="https://plus.google.com/" target="_blank"><img src="~/images/gmail-footer.png" /></a></div>
                </div>


            </div>
        </div>
    </div>
</body>
</html>


<script>
    

    $(document).ready(function () {
        $('#close').click(function () {

            $('#myModal').modal('hide');



        });

        $('#closeSignup').click(function () {

            $('#signupModal').modal('hide');



        });
    });

</script>
<script>
    $(document).ready(function () {
        $('#b7').click(function () {

            $('#div1').load('@Url.Action("AboutUs", "User")');

        });
    });
  
</script>